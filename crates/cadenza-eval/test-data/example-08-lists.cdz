# Lists
# Collection of values in square brackets

# Empty list
[]

# Simple list of integers
[1, 2, 3, 4, 5]

# List of mixed types
[42, 3.14, "hello"]

# Assign to variable
let numbers = [10, 20, 30]
numbers

# List with expressions
let x = 5
let y = 10
[x, y, x + y, x * y]

# Nested lists
[[1, 2], [3, 4], [5, 6]]

# List with computed values
let a = 100
let b = 200
[[a, a * 2], [b, b / 2]]
